{
  "name": "my-app",
  "version": "0.4.0",
  "author": "Steve Simkins",
  "devDependencies": {
    "bun-types": "latest",
    "turbo": "^2.5.5"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "description": "A monorepo template built with Bun, Hono, Vite, and React",
  "homepage": "https://github.com/stevedylandev/bhvr",
  "keywords": [
    "bun",
    "hono",
    "react",
    "vite",
    "monorepo",
    "turbo"
  ],
  "license": "MIT",
  "packageManager": "bun@1.2.4",
  "scripts": {
    "dev": "turbo dev",
    "dev:client": "turbo dev --filter=client",
    "dev:server": "turbo dev --filter=server",
    "build": "turbo build",
    "build:client": "turbo build --filter=client",
    "build:server": "turbo build --filter=server",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "postinstall": "turbo build  --filter=server",
    "db:push": "turbo -F server push",
    "db:studio": "turbo -F server db:studio",
    "db:generate": "turbo -F server db:generate",
    "db:migrate": "turbo -F server db:migrate"
  },
  "workspaces": [
    "./server",
    "./client"
  ],
  "dependencies": {
    "bgg-client": "^1.1.2"
  }
}
